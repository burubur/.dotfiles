# git
alias grm="rm -rf .git"

# application
alias loadalias="source $HOME/.dotfiles/.alias"
alias h="htop"
alias chrome="/Applications/Google\ Chrome.app/Contents/MacOS/Google\ Chrome"
alias code="/usr/local/bin/code-insiders"
alias co="/usr/local/bin/code ."
alias insider="/Applications/Visual\ Studio\ Code\ -\ Insiders.app/Contents/MacOS/Electron ;"
alias server="python3 -m http.server 3000"
alias servero="python3 -m http.server 3000 && open http://localhost:3000"
alias lr="lein repl"
alias rping="redis-cli ping"
alias tf="~/.bin/terraform"
alias k="kubectl"
alias kubectx="kubectl get current-context"
alias kx="kubectx"
alias tf="terraform"
alias tfi="terraform init"
alias tfa="terraform apply"
alias tfd="terraform destroy"
alias tfr="terraform refresh"
alias tfo="tf output"
alias tfj="tf output -json"

# golang
alias gob="go build"
alias gt="go test ./..."
alias dl="cd $HOME/Downloads"
alias godir="cd $HOME/lab/go/src/bitbucket.org/kudoindonesia"
alias gorun="go run ./main.go"
alias labgo="cd $HOME/lab/go/source"
alias labjs="cd $HOME/lab/js"
alias labjava="cd $HOME/lab/java"
alias labruby="cd $HOME/lab/ruby"
alias labclo="cd $HOME/lab/clojure"
alias labnode="cd $HOME/lab/node"
alias laband="cd $HOME/lab/android"
alias labdesign="cd $HOME/lab/design"
alias labdoc="cd $HOME/lab/doc"
alias gos="cd ~/lab/go/src"
alias cov="go test ./... -coverprofile=c.out && go tool cover -html=c.out"
alias runmock="go run ~//lab/go/src/bitbucket.org/kudoindonesia/mocking_biller/cmd/mocking_biller/main.go serve"
alias rundjipayment="go run ~/lab/go/src/bitbucket.org/kudoindonesia/biller_connector_dji/main.go payment-processor --config $HOME/lab/go/src/bitbucket.org/kudoindonesia/biller_connector_dji/configuration/app.yaml"
alias p="go run ~/lab/go/src/github.com/burubur/kafka-producer/main.go"

# docker
alias dcl="docker container list"
alias dvl="docker volume list"
alias dvp="docker volume prune"
alias dcp="docker container prune"
alias dil="docker images"
alias dip="docker image prune"
alias dsp="docker system prune"
alias dcu="docker-compose up -d"
alias dcstop="docker-compose stop"
alias dcdown="docker-compose down"
alias dcs="docker container stop"
alias dck="docker container kill"
alias runpg="docker run --rm -d --name postgres -e POSTGRES_PASSWORD=secret -p 15432:5432 -v $HOME/volumes/postgres:/var/lib/postgresql/data postgres"
alias runrabbit="docker run -d --hostname rabbit --name rabbit rabbitmq"
alias runpgcls="docker run -d --name postgres --rm -p 127.0.0.1:5432:5432 cls-postgres"
alias runredis="redis-server /usr/local/etc/redis.conf --daemonize yes"
alias killredis="redis-cli shutdown"
alias runkafka="docker-compose up -d -f ~/lab/kafka/containerized-kafka/docker-compose.yaml"

alias findport="sudo lsof -iTCP -sTCP:LISTEN -n -P | grep"
alias portusages="sudo lsof -iTCP -sTCP:LISTEN -n -P"
alias usedport="lsof -n -P -i | grep"

# tmux
alias t="tmux a -t base"
alias tl="tmux ls"
alias ts="tmux source ~/.tmux.conf"

# vim
alias te="vim .tmux.conf"
alias ve="vim ~/.vimrc"

# npm
alias ni="npm install"
alias nu="npm uninstall"
alias nup="npm update"
alias nri="rm -r node_modules && npm install"
alias ncd="npm-check -su"
alias redir="cd ~/lab/js/react"

# utils
alias l="ls -la"
alias _="sudo"
alias rr="rm -rf"
alias ez="vim ~/.zshrc"
alias sz="source ~/.zshrc"
alias e="exit"
alias c="clear"
alias eh="sudo vim /etc/hosts"
alias ch="cat /etc/hosts"
alias copypath="pwd|pbcopy"

